<!DOCTYPE html>
<html>
<head>
	<title>Sentiments</title>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>

	<%= stylesheet_link_tag "materialize.min.css" %>
	<%= stylesheet_link_tag "styles.css" %>

	<!-- Compiled and minified JavaScript -->
	<%= javascript_include_tag 'jquery-2.1.1.min.js' %>
	<%= javascript_include_tag 'materialize.min.js' %>
	<%= javascript_include_tag 'highcharts.js' %>
</head>
<body>

	<%= render 'partials/nav' %>

	<%= yield %>


	<script>
      $(document).ready(function(){
        $('select').material_select();
        $('input.autocomplete').autocomplete({
	    data: {
	      "Google (GOOG)": null,
	      "Credit Suisse (CS)": null,
	      "Facebook (FB)": null,
	      "Yahoo (YHOO)": null,
	      "Tableau Software (DATA)": null,
	      "Thomson Reuters Inc. (TRI)": null,
	      "Goldman Sachs (GS)": null,
	      "Amazon (AMZN)": null,
	      "Cloudera (CLDR)": null
	    },
	    limit: 20, // The max amount of results that can be shown at once. Default: Infinity.
	    onAutocomplete: function(val) {
	      // Callback function when value is autocompleted.
	    },
	    minLength: 1, // The minimum length of the input for the autocomplete to start. Default: 1.
	    });
	    Materialize.updateTextFields();
      });

    
    </script>
</body>
</html>
